    <!-- World Coffee Research -->
    <div class="grid grid-cols-1 lg:grid-cols-2 gap-[30px] lg:gap-[52px]">
        <div class="transparency--card bg-[#E0DF00] border border-black px-4 xl:px-[52px] py-5 xl:py-[46px] flex flex-col justify-between">
            <img class="rounded-[30px] mb-6 lg:mb-8" src="{{ section.settings.wcr_image_1 |  img_url: 'master' }}" alt="coffee card 1">

            <h4 class="text-[30px] lg:text-[36px] font-acorn-medium mb-4 leading-tight text-center lg:text-left w-4/5 mx-auto  lg:w-full">{{ section.settings.wcr_header }}</h4>
            <div class=" text-lg font-light mb-9 lg:mb-[60px] text-center lg:text-left">
                {{ section.settings.wcr_header_1 }}
            </div>

            <a href=" {{ section.settings.wcr_url }}" target="_blank" class="btn btn-secondary text-base font-medium w-full lg:w-fit">LEARN MORE</a>
        </div>

        <div class="transparency--card bg-[#DDBFFC] border border-black px-4 xl:px-[52px] py-5 xl:py-[46px] flex flex-col justify-between">

            <div class="flex flex-col gap-8 items-center">

                <img class="w-full mx-auto" src="{{ section.settings.wcr_image_2 |  img_url: 'master' }}" alt="coffee card 2">
                {% comment %} 
                <div class="hidden lg:block">
                    <!-- Flower Image -->
                    <img class="w-[98px] mx-auto" src="https://cdn.shopify.com/s/files/1/0249/4283/7837/files/bloom_fan.gif?v=1718022630" alt="bloom fan">
                </div> {% endcomment %}

            </div>

            <div class="text-lg font-medium py-6 lg:py-0 text-center lg:text-left">
                {{ section.settings.wcr_header_2 }}
            </div>


            <div class="lg:hidden_ mx-auto_ w-fit">
                <!-- Flower Image: Mobile -->
                <img class="w-[98px]" src="https://cdn.shopify.com/s/files/1/0249/4283/7837/files/bloom_fan.gif?v=1718022630" alt="bloom fan">

            </div>



            
        </div>

    </div>


</section>



