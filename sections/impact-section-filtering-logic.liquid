    // Add the Products data to each producer
    $('.producer-country__partner').each(function(index) {
        let partnerMetaData = $(this).find('script[data-producer-info]');
        let partnerInfo = JSON.parse(partnerMetaData.html());

        console.log('products from the latest year ==> ', partnerInfo.producerInfo[0]['Sold As'])

        let firstSoldAsDataElem = $(this).find('.pp__sold-as-data');
        let firstSoldAsDataElemPopup = $(this).find('.pp__sold-as-data.pp-prod-data--popup');
        //console.log('Sold As Data', index, '====>', firstSoldAsDataElem)

        let firstProdData = '';
        let firstProdDataPopup = '';

        let firstSoldAsProds = partnerInfo.producerInfo[0]['Sold As'];
        let fetchPromises = []; // Array to store fetch promises
            firstSoldAsProds.forEach(productHandle => {
                let fetchPromise = fetch(window.Shopify.routes.root + 'products/' + productHandle + '.js')
                    .then(response => {
                        if (response.ok) {
                            return response.json();
                        } 
                        else {
                            throw new Error(`Product handle not found: ${productHandle}`);
                        }
                    })
                    .then(product => {
                        //console.log('product ===> ', product)
                        let productTitleFromHandle = product.title; // Assign the title to the variable
                        let productPriceFromHandle = product.price;
                        let productImageFromHandle = product.featured_image;
                        const formattedPrice = (productPriceFromHandle / 100).toFixed(2);
                        firstProdData += `
                            <a href="/products/${productHandle}" target="_blank" class="inline-block border border-black px-3 py-1 leading-none rounded-full">
                                ${productTitleFromHandle}
                            </a>
                        `;
                        firstProdDataPopup += `
                            <a href="/products/${productHandle}" target="_blank" class="block border border-black p-3 leading-none rounded-[14px]">
                               <img class="w-4/5 mx-auto mb-2 min-h-[48px]" src="${productImageFromHandle}"  />
                               <p class="text-sm lg:text-base font-acorn-semibold text-center leading-none mb-2"> ${productTitleFromHandle}</p>
                               <p class="text-sm font-acorn-semibold text-center leading-none"> $${formattedPrice}</p>
                            </a>
                        `;
                    })
                    .catch(error => {
                        console.error('Error fetching product:', error);
                    });
                fetchPromises.push(fetchPromise); // Store the fetch promise
            });
            // Wait for all fetch requests to complete
            Promise.all(fetchPromises)
                .then(() => {
                    // Update the HTML with product titles after all fetch requests are finished
                    firstSoldAsDataElem.fadeOut(200, function() {
                        $(this).html(firstProdData).fadeIn(200);
                    });

                    firstSoldAsDataElemPopup.fadeOut(200, function() {
                        $(this).html(firstProdDataPopup).fadeIn(200);
                    });

                    console.log('==== UPDATED INITIAL PRODUCT LIST =====')
                });




    })


        




