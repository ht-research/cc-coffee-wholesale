    //Country Filters
    $('.producer-country__index').each(function(){
        $(this).on('click', function() {
           

            $(this).siblings().removeClass('pp-country--active')
            $(this).addClass('pp-country--active')
            


            let selectedCountryIndex = $(this).attr('data-country-index');
            console.log(selectedCountryIndex)

            //For Mobile
            $(this).parents().siblings('.producer-country__index--main').children('span').html(selectedCountryIndex)
            $(this).parents().siblings('.producer-country__index--main').trigger('click')




            let allCountryPartners = $('.producer-country__partner')
            let selectedCountryPartners = $('.producer-country__partner').filter(`[data-country-name="${selectedCountryIndex}"]`);
            //console.log(selectedCountryPartners)


            if(selectedCountryIndex == 'All'){
                allCountryPartners.removeClass('hidden')
            }
            else{
                allCountryPartners.addClass('hidden')
                selectedCountryPartners.removeClass('hidden').addClass('flex')
            }
            


        })
