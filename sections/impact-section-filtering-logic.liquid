            qualityScoreDataElem.fadeOut(200, function() {
                $(this).text(producerInfo.producerInfo[currentSelectedDropdownIndex - 1]['Quality Score']).fadeIn(200);
            });


            let soldAsDataElem = $(this).parents().parents('.producer-year__dropdown').parents().siblings('.producer-year__data').find('.pp__sold-as-data');
            let soldAsProdData = '';
         
            let allSoldAsProds = producerInfo.producerInfo[currentSelectedDropdownIndex - 1]['Sold As']
            console.log(allSoldAsProds)


            if($(this).hasClass('producer-dropdown-product--popup')){
                console.log('Producer Popup Year Clicked')
                soldAsDataElem = $(this).parents().parents('.producer-year__dropdown').siblings('.pp__sold-as-data.pp-prod-data--popup');
                console.log(soldAsDataElem)
                $(this).parents().siblings('.sus--year-selector').trigger('click')
            }
        


            let fetchPromises = []; // Array to store fetch promises
            allSoldAsProds.forEach(productHandle => {
                let fetchPromise = fetch(window.Shopify.routes.root + 'products/' + productHandle + '.js')
                    .then(response => {
                        if (response.ok) {
                            return response.json();
                        } 
                        else {
                            throw new Error(`Product handle not found: ${productHandle}`);
                        }
                    })
                    .then(product => {
                        let productTitleFromHandle = product.title; // Assign the title to the variable
                        let productPriceFromHandle = product.price;
                        let productImageFromHandle = product.featured_image;
                        const formattedPrice = (productPriceFromHandle / 100).toFixed(2);
                        if($(this).hasClass('producer-dropdown-product--popup')){
                            soldAsProdData += `
                            <a href="/products/${productHandle}" target="_blank" class="block border border-black p-3 leading-none rounded-[14px]">
                               <img class="w-4/5 mx-auto mb-2 min-h-[48px]" src="${productImageFromHandle}"  />
                               <p class="text-sm lg:text-base font-acorn-semibold text-center leading-none mb-2"> ${productTitleFromHandle}</p>
                               <p class="text-sm font-acorn-semibold text-center leading-none"> $${formattedPrice}</p>
                            </a>
                        `;
                        }
