<script>  
  const stickyElement = document.getElementById('desktopCountryStickyTab');
//   console.log(stickyElement);

// Listen for scroll events
window.addEventListener('scroll', () => {
      const rect = stickyElement.getBoundingClientRect();

      // Check if the element is stuck at 78px from the top of the viewport
      if (rect.top <= 78) {
        stickyElement.classList.add('active'); // Apply 'active' class when sticky
        //console.log('Element is stuck');
      } else {
        stickyElement.classList.remove('active'); // Remove 'active' class when not sticky
